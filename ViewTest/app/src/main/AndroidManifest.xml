<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.android.discovery.viewtest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoTitle"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".Activity.SearchActivity"></activity>
        <activity android:name=".Activity.BorrowDetailActivity" />
        <activity android:name=".Activity.BorrowListActivity" />
        <activity android:name=".Activity.BookDetailActivity" />
        <activity
            android:name=".Activity.ForgetActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.RegActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.LoginActivity"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            tools:ignore="InnerclassSeparator">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->
            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
    </application>

</manifest>